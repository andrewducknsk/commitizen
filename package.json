{
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-customizable": "6.3.0",
    "husky": "^7.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.config.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.js"
    }
  },
  "dependencies": {
    "@commitlint/cli": "^15.0.0"
  }
}
